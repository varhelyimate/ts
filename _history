ding.play()\n    game.splash(\"Nehézség növekszik!\")\n    for i in range(3):\n        spawn_enemy()\ngame.on_update_interval(12000, harder)\n","pxt.json":"{\n    \"name\": \"ötös\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"device\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"assets.json\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}\n"}}],"shares":[],"lastSaveTime":1763037322012}